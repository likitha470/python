list=["banana","lichi","apple"] #list append
list.append("5")
print(list)#none
list.sort()
print(list)
list.sort(reverse=True)#return to none
print(list)
list.reverse()
print(list)
list.insert(1,3)
list.insert(0,"4")
print(list)
list.remove("4")#remove the element##
print(list)
list.pop(3)#remove the index
print(list)
